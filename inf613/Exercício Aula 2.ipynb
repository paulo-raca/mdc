    "k = c()\n",
    "err = c()\n",
    "for (i in 1:10) {\n",
    "    set.seed(1234)\n",
    "    r <- kmeans(Boston, i)    \n",
    "    k = c(k, i)\n",
    "    err = c(err, r$tot.withinss)\n",
    "}\n",
    "all <- data.frame(k, err)\n",
    "all"
